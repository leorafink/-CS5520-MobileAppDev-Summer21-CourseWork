<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.numad21su_leorafink">
    
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.INTERNET"/>


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_lf"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_lf_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.NUMAD21SuLeoraFink">
        <activity android:name=".MainActivity"
                  android:configChanges="orientation|screenSize|screenLayout|keyboardHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".RecycleActivity"
            android:configChanges="orientation|screenSize|screenLayout|keyboardHidden">
        </activity>

        <activity android:name=".ButtonActivity"
            android:configChanges="orientation|screenSize|screenLayout|keyboardHidden">
        </activity>
        <activity android:name=".LocatorActivity"
            android:configChanges="orientation|screenSize|screenLayout|keyboardHidden">
        </activity>
        <activity android:name=".WebServiceActivity"
            android:configChanges="orientation|screenSize|screenLayout|keyboardHidden">
        </activity>
    </application>

</manifest>